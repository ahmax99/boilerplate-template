{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "incremental": true,
    "noFallthroughCasesInSwitch": false,
    "removeComments": true,
    "sourceMap": true,
    "strictBindCallApply": false
  }
}
